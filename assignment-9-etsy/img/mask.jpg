body {
  margin: 0px;
  padding: 0px;
  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
}
.header-container{
display: flex;
align-items: center;
padding-top: 1%;
padding-left: 2%;
padding-right: 2%;
}
.search {
  display: flex;
  align-items: center;
  border: 1px solid #e0e0e0;
  border-radius: 40px;
  width: 78%;
  padding: 4px 12px;
  background-color: #eeeeee;
  margin: 0% 1%;
}
.searchbar {
  width: 145%;
}
.searchinput {
  outline: none;
  border: none;
  width: 95%;
  background-color: #eeeeee;
  font-size: 3vh;
}
.searchlens .large.material-icons {
  font-size: 2rem;
  color: black;
}
.header-signin a {
  text-decoration: none;
  color: black;
  font-weight: bold;
}
.header-signin {
  margin: 0% 2%;
  width: 52px;
}

.searchlens .large.material-icons:hover {
  color: white;
}

.header-row-two {
  border-bottom: 2px solid  #e0e0e0;
  padding: 0.5% 2%;
}
.header-row-two ul {
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0.7% 0;
  padding: 0px;
}
.header-row-two a {
  text-decoration: none;
  color: black;
}
.header-row-two li {
  list-style-type: none;
  margin: 0 1%;
}
.main-section-heading {
  text-align: center;
}
.main-section-heading p {
  font-size: 34px;
  margin: 0;
  padding: 25px 10px;
}
.row1 {
  display: flex;
  align-items: center;
  padding: 0 2%;
}

.row1-main {
  display: flex;
  height: 320px;
  width: 65%;
  margin-right: 1.6%;
  background-color: 1px solid #eeeeee;
  border-radius: 8px;
  border: 1px solid #eeeeee;
  border-radius: 8px;
}
.row1-text {
  display: flex;
  flex-flow: column;
  justify-content: center;
  background-color: #ddebe3;
  width: 28%;
  height: 320px;
  padding: 0 1.5%;
}
.row1-text-box1 a {
  text-decoration: none;
  color: black;
  font-size: 27px;
}
.row1-text-box1 {
  text-align: center;
}
.row1-text-box2 {
  text-align: center;
  margin: 2% 0;
}
.shop-now {
  text-decoration: none;
  color: black;
  font-weight: bold;
}
.row1-image {
  height: 320px;
  width: 69%;
}
.row1-image img {
  height: 320px;
  width: 100%;
}
.row1-sidebox {
  height: 320px;
  width: 33.5%;
  position: relative;
}
.mask-image {
  height: 220px;
  background-color: white;
  margin-bottom: 4%;
  border-radius: 8px;
  text-align: right;
  width: 100%;
  border: 1px solid #eeeeee;
}
.mask-image img {
  height: 220px;
  width: 70%;
  border-radius: 8px;
}

.mask-image-text {
  background-color: #ddebe3;
  height: 83px;
  border-radius: 10px;
  display: flex;
  align-items: center;
  justify-content: center;
  border: 1px solid #e0e0e0;
  border-radius: 8px;
}
.mask-image-text a {
  text-decoration: none;
  color: black;
  padding: 0 5px;
  font-weight: bold;
}
.mask-banner p {
 margin: 0;
 font-size: 22px;
 padding: 15px 0px;
 font-weight: bold;
 
}
.mask-banner {
  width: 200px;
  position: absolute;
  right: 48%;
  top: 1%;
  /* margin-left: 212%;
  margin-top: 4%; */
  z-index: 5;
}
.mask-banner a {
  text-decoration: none;
  color: black;
  font-weight: bold;
  padding: 5px 0px;
 }
 .row2{
   display: flex;
   padding: 2% 2%;
   align-items: center;
   justify-content: space-around;
 }
 .row2 a {
   text-decoration: none;
   color: black;
   font-size: 17px;
   font-weight: 200;
 }
 .secondrow {
   width: 26%;
   text-align: center;
 }
 .box1 {
   margin-right: 7%;
 }
 .box2 {
  margin-right: 7%;
}
.row3 {
  padding: 0 2%;
  text-align: center;
  
}
.row3-heading {
  margin-bottom: 2%;
}
.row3-heading p {
  margin: 0;
  font-size: 27px;
  font-weight: bold;
}
.row3-container {
  display: flex;
  margin-bottom: 3%;
}
.thirdrow {
  width: 20%;
  text-align: justify;
  padding: 0 2px;
}
.row3-container a {
  text-decoration: none;
  color: black;
}
abbr[title] {
  text-decoration: none;
}
.seller {
  opacity: 0.7;
  font-size: 12px;
}
.row3-container .info {
  font-size: 13px;
  color: red;
}
.row-4 {
